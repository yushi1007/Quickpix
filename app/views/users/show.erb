
<div class="image-page">
    <h1><%= @user.name %>'s images</h1>
        <%= link_to "My Account", edit_user_path(@user)%>
        <%= link_to "Upload", new_image_path %>
        <%= link_to "Explore", images_path%>
    <div class="image-container">
        <% @user.images.each do |user|%>
        <!--<img src = <%= user.url %> width="300" height="300" >-->
        <div class="image">
        <%= link_to image_tag(user.url), user%>
        </div>
        <%end%>
        <hr>
    </div>
</div>


